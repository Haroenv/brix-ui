{
  "name": "@ustudio-ui/theme",
  "version": "0.0.1",
  "description": "Theme package providing light and dark modes as well as css/js variables and helpers used in theming",
  "keywords": ["theme", "ui", "light mode", "dark mode", "css", "styled-components", "css-in-js"],
  "scripts": {
    "tsconfig": "node ../../scripts/typescript.js",
    "package": "node ../../scripts/package.js",
    "prebuild": "copyfiles README.md LICENSE lib",
    "build": "babel src --out-dir lib --extensions \".ts,.tsx\" --copy-files"
  },
  "dependencies": {
    "@ustudio-ui/types": "0.0.1",
    "@ustudio-ui/utils": "0.0.1",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/lodash.merge": "^4.6.6",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.1",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "polished": "^3.6.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  }
}
